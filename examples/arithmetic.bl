# Arithmetic test
MOV R0, 24
MOV R1, 7

# Addition
PRINT "24 + 7 = "
MOV R2, R0
ADD R2, R1
PUTDEC R2
PUTCHAR 10

# Subtraction
PRINT "24 - 7 = "
MOV R2, R0
SUB R2, R1
PUTDEC R2
PUTCHAR 10

# Multiplication
PRINT "24 * 7 = "
MOV R2, R0
MUL R2, R1
PUTDEC R2
PUTCHAR 10

# Division
PRINT "24 / 7 = "
MOV R2, R0
DIV R2, R1
PUTDEC R2
PUTCHAR 10
